$ docker build -t <Image Name> .

$ docker run --rm -it <Image Name> /bin/bash

$ docker run -p 8888:8888 -v "$PWD/notebooks:/opt/notebooks" --name <Container Name: eg. data-science> <Image Name: eg. data-science>:<Image Tag: eg. latest>

